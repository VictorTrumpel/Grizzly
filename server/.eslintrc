{
  "extends": [
      "plugin:import/recommended",
      "plugin:import/typescript",
      "prettier"
  ],
  "rules": {
    "import/order": ["error", {
      "alphabetize": {
        "order": "asc"
      },
      "groups": ["builtin", "external", "internal", ["parent", "sibling"]],
      "newlines-between": "always",
      "pathGroups": [
        {"pattern": "@/**", "group": "internal"}
      ]
    }]
  }
}
